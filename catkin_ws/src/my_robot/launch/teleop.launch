<?xml version="1.0"?>
<launch>

  <!-- linear and angular speed -->
  <arg name="linear" default="_speed:=0.7"/>
  <arg name="angular" default="_turn:=2.1"/>

  <node name="teleop" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen" args="$(arg linear) $(arg angular)"/>

</launch>